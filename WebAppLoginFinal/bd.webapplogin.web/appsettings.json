{

  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning"
    }
  },


  //Nombre del servicio de log que tiene que ser igual al nombre que tiene este en la base de datos

  "ServiciosLog": "LogWebService",
  //Configuración del tiempo de vida de la cookie Horas-Minutos-Segundos
  "TiempoVidaCookieHoras": 0,
  "TiempoVidaCookieMinutos": 10,
  "TiempoVidaCookieSegundos": 10,

  //Host de servicio de seguridad tiene que ser la dirección
  //donde se encuentra el servicio de seguridad

  //"HostServicioSeguridad": "http://192.168.100.21/swSeguridad"
  "HostServicioSeguridad": "http://localhost/swSeguridad",
  //"HostServicioSeguridad": "http://localhost:53317",


  //Configuración del Active Directory
  "ldap": {
    "url": "ldap.local",
    "bindDn": "CN=user,OU=branch,DC=contoso,DC=local",
    "bindCredentials": "secret_password",
    "searchBase": "DC=contoso,DC=local",
    "searchFilter": "(&(objectClass=user)(objectClass=person)(sAMAccountName={0}))",
    "adminCn": "CN=Admins,OU=branch,DC=contoso,DC=local"
  }

}
